<!DOCTYPE html>
<html
	itemscope
	itemtype="http://schema.org/WebPage"
	{{ site.language_attributes }}
	{{ html_class( 'no-js no-touch' ~ ( 'front-page' == namespace ? ' h-100 w-100' ) ~ ( 'horizontal-page' in post.modules ? ' cursor-wait' ) ) }}
>

	<head>

		<meta charset="{{ site.charset }}">
		<link rel="profile" href="http://gmpg.org/xfn/11">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=5">

		{{ wp_head }}

		{# {% include 'components/gtag.html.twig' with { id: 'UA-153672608-1' } %} #}

    </head>


    <body class="{{ body_class }} position-relative fw-normal ff-body {{ post.body_classes ? ( ' ' ~ post.body_classes | join( ' ' ) ) }}">
		<div id="svg-sprite" class="d-none">{% include 'icons.svg' %}</div>

