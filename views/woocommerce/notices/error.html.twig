{% if notices is iterable and notices is not empty %}

	<ul class="Error shadow-uni woocommerce-error list-style-type-none border-radius-5px background-color-red-light-grayish-01 color-red-very-dark-01" role="alert">
		{% for notice in notices %}
			<li{{ fn( 'wc_get_notice_data_attr', notice ) }}>
				{{- notice.notice -}}
			</li>
		{% endfor %}
	</ul>

{% endif %}
