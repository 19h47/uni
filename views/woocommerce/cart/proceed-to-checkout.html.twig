{#
 # Cart: Proceed to checkout
 #
 # Contains the markup for the proceed to checkout button on the cart.
 #
 # @package UNI
 #}

{{-
	include(
		'components/button.html.twig',
		{
			tag: 'a',
			label: __( 'Proceed to checkout', 'uni' ),
			classes: [ 'alt', 'wc-forward', 'inline-block', 'w-100', fn( 'wc_wp_theme_get_element_class_name', 'button' ) ],
			attributes: {
				href: checkout_url | e( 'esc_url' )
			}
		}
	)
-}}
