{#
 # Panel
 #}

<div
	id="{{ id | default( '' ) }}"
	class="transition-transform z-4 inset-0 fixed Menu pt-100px pt-md-20px pb-20px px-20px ps-md-100px"
	style="transform: translate3d(100%, 0, 0);"
	data-module-panel="{{ id | default( '' ) }}"
	{{ attributes ? attributes | map( ( value, key ) => "#{key}=\"#{value}\"" ) | join( ' ' ) | raw }}
>
	<div class="bg-body bg-opacity-97 shadow-uni rounded h-100">
		{% block content %}{% endblock %}
	</div>
</div>
